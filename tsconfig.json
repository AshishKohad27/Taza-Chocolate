{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "config/db.ts", "pages/products/index.tsx", "constants/product.ts", "pages/api/cart/index.ts", "middleware/authMiddleWare.js", "components/Footer/Footer.jsx", "pages/collections/[id].jsx", "components/Product/ProductFooter2.jsx", "components/Product/ProductFooter.jsx", "pages/collections/index.jsx", "pages/index.jsx"],
  "exclude": ["node_modules"]
}
